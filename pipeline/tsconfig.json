{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"noUncheckedIndexedAccess": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", "agent-data"]
}
